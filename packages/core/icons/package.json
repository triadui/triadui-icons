{
  "name": "@triadui/icons",
  "version": "0.1.2",
  "description": "Core Material Design 3 icon component for React applications with TypeScript and JavaScript support. Works with Next.js, Vite, CRA, and more. Part of Triad UI.",
  "type": "module",
  "main": "./dist/triadui-icons.cjs",
  "module": "./dist/triadui-icons.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/triadui-icons.cjs",
      "default": "./src/index.ts"
    },
    "./styles": "./src/styles.css"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && vite build",
    "build:lib": "tsc --project tsconfig.build.json && vite build --config vite.config.ts",
    "lint": "eslint .",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  },
  "keywords": [
    "react",
    "icons",
    "material-symbols",
    "material-design",
    "typescript",
    "javascript",
    "nextjs",
    "next.js",
    "vite",
    "components",
    "ui",
    "material-3",
    "m3",
    "variable-fonts",
    "icon-library",
    "font-icons",
    "esm",
    "cjs",
    "commonjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triadui/triadui-icons.git",
    "directory": "packages/core/icons"
  },
  "bugs": {
    "url": "https://github.com/triadui/triadui-icons/issues"
  },
  "homepage": "https://github.com/triadui/triadui-icons#readme"
}
