{
  "name": "@triadui/icons",
  "version": "0.1.0",
  "description": "Core Material Design 3 icons component for React with TypeScript support",
  "type": "module",
  "main": "dist/triadui-icons-core.umd.js",
  "module": "dist/triadui-icons-core.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/triadui-icons-core.es.js",
      "require": "./dist/triadui-icons-core.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && vite build",
    "build:lib": "tsc --project tsconfig.build.json && vite build --config vite.config.ts",
    "lint": "eslint .",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  },
  "keywords": [
    "react",
    "icons",
    "material-symbols",
    "material-design",
    "typescript",
    "components",
    "ui",
    "material-3",
    "m3",
    "variable-fonts",
    "icon-library",
    "font-icons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/mui-iii.git",
    "directory": "packages/core/icons"
  },
  "bugs": {
    "url": "https://github.com/your-username/mui-iii/issues"
  },
  "homepage": "https://github.com/your-username/mui-iii#readme"
}
